# FILE_SERVER_ADDRESS=$FILE_USER

# MAIL SERVER
export MAIL_USER=""
export MAIL_ADDRESS=""
export MAIL_SERVER_ADDRESS="$MAIL_USER@$MAIL_ADDRESS"

export MRG_USERNAME=""
export MRG_PASSWORD=""

